{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2020, // ecmaVersion 2020을 쓸경우 env 옵션에도 es2020: true 설정으로 맞춤
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env":{
        "browser": true,
        "node": true,
        "es2020":true
    },
    "extends": [
        "aribnb"
    ],
    "plugins": [
        "import",
        "react-hooks"
    ],
    "rules": {
        "jsx-a11y/label-has-associated-control": "off",
        "jsx-a11y/anchor-is-valid": "off",
        "no-console": "off",
        "no-underscore-dangle": "off",
        "react/forbid-prop-types":"off",
        "react/jsx-filename-extension":"off",
        "react/jsx-one-expression-per-line":"off",
        "object-curly-newline": "off",
        "linebreak-style": "off"
    }
}